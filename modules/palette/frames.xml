<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

	<Frame name="ColorToolsColorPalette" parent="ColorPickerFrame" virtual="true">

		<Anchors>
			<Anchor point="TOPLEFT" x="25" y="-190"/>
			<Anchor point="BOTTOMRIGHT" x="-25" y="30"/>
		</Anchors>

		<Layers>
			<Layer level="ARTWORK">
				<Texture>
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
					<Color r="1.0" g="1.0" b="1.0" a="0.0"/>
				</Texture>
			</Layer>
		</Layers>
		
	</Frame>


	
	<Button name="ColorToolsColorButton" virtual="true" >
		<Size x="32" y="32"/>
		<Anchors>
			<Anchor point="TOPLEFT" x="0" y="0"/>
		</Anchors>
	</Button>



</Ui>