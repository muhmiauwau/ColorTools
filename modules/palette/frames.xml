<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

	


	
	<Button name="ColorToolsColorButton" virtual="true" >
		<Size x="32" y="32"/>
		<Anchors>
			<Anchor point="TOPLEFT" x="0" y="0"/>
		</Anchors>
	</Button>


	<ScrollFrame name="ColorToolsPaletteScrollFrame" parent="ColorPickerFrame" inherits="UIPanelScrollFrameCodeTemplate">
		<Anchors>
			<Anchor point="BOTTOMLEFT" x="23" y="44"/>
			<Anchor point="BOTTOMRIGHT" x="-33" y="44"/>
		</Anchors>
		<ScrollChild>
			<Frame parentKey="Contents" parent="ColorToolsPaletteScrollFrame" >
				<Anchors>
					<Anchor point="TOPLEFT" x="0" y="0"/>
					<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
				</Anchors>
			</Frame>
		</ScrollChild>
		<Scripts>
			<OnLoad>
				self.scrollBarHideable = true;
				ScrollFrame_OnLoad(self);
			</OnLoad>
		</Scripts>
	</ScrollFrame>





</Ui>